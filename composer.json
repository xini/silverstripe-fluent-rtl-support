{
	"name": "innoweb/silverstripe-fluent-rtl-support",
	"description": "Adds RTL support to CMS fields of sites that use fluent.",
	"type": "silverstripe-vendormodule",
	"license": "BSD-3-Clause",
	"keywords": ["silverstripe", "fluent", "rtl", "cms", "language", "translation", "arabic"],
	"authors": [{
		"name": "Florian Thoma",
		"homepage": "https://www.innoweb.com.au"
	}],
	"require": {
		"silverstripe/cms": "^4",
		"tractorcow/silverstripe-fluent": "^4 || ^5"
	},
	"autoload": {
		"psr-4": {
			"Innoweb\\FluentRTLSupport\\": "src/"
		}
	},
	"extra": {
		"expose": [
			"client/dist"
		]
	}
}
